function setup()
    ga.wave(GA_TRI,120)    
end

function update()
    
end

function draw()
    ga.background(0.0,1.0)
end

function keyReleased()
    ga.waveFrequency(0,(ga.key()/3)*ga.nToF(RE_0))
    ga.waveVolume(0,of.randomuf()/4)
end
