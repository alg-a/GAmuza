/* GAmuza 0.4.1 examples  --------------------- Basics/noiseMap.ga  Basic use of noise generator. created by mj*/v = 0.0inc= 0.01function setup()  	ofEnableSmoothing()endfunction update()	v += inc  endfunction draw()	gaBackground(0.0, 0.05)		ofSetColor(255)	x = ofMap(ofNoise(v),0.15,0.85,0,OUTPUT_W, false)	y= (OUTPUT_H/2) + ofRandom(-4,4)	ofCircle(x,y,16,16)   end